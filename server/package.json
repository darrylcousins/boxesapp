{
  "name": "boxesapp",
  "version": "0.0.1",
  "description": "A boxes app from shopify",
  "private": true,
  "scripts": {
    "build": "npm run build:client",
    "build:client": "vite build --config config/vite.js --outDir dist/client",
    "debug": "node --inspect-brk src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --watch ./src",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production node src/index.js",
    "start": "npm run serve",
    "test": "SHOP=test-shop.myshopify.com vitest --config config/vite.js --reporter=verbose"
  },
  "nodemonConfig": {
    "ext": "js,jsx,html,scss,json"
  },
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/darrylcousins/boxesapp.git"
  },
  "keywords": [
    "shopify"
  ],
  "author": "Darryl Cousins <darryljcousins@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darrylcousins/boxesapp/issues"
  },
  "homepage": "https://github.com/darrylcousins/boxesapp#readme",
  "devDependencies": {
    "@b9g/crank": "^0.4.3",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cross-env": "^7.0.3",
    "crossroads": "^0.12.2",
    "dotenv": "^16.0.1",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "highlight.js": "^11.6.0",
    "isomorphic-fetch": "^3.0.0",
    "liquidjs": "^9.39.2",
    "lodash.isequal": "^4.5.0",
    "markdown-it": "^13.0.1",
    "mjml": "^4.13.0",
    "mongo-mock": "^4.1.0",
    "mongodb": "^4.8.1",
    "morgan": "^1.10.0",
    "msw": "^0.44.2",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "promise-throttle": "^1.1.2",
    "sass": "^1.54.0",
    "serve-static": "^1.15.0",
    "supertest": "^6.2.4",
    "tachyons": "^4.12.0",
    "vite": "^2.9.9",
    "vitest": "^0.19.1",
    "winston": "^3.8.1",
    "winston-mongodb": "^5.0.7"
  }
}

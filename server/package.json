{
  "name": "boxesapp",
  "version": "0.0.1",
  "description": "A boxes app from shopify",
  "private": true,
  "scripts": {
    "build": "npm run build:client",
    "build:client": "vite build --config config/vite.js --outDir dist/client",
    "debug": "node --inspect-brk src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --watch ./src",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production node src/index.js",
    "start": "npm run serve",
    "test": "vitest --config config/vite.js --reporter=verbose"
  },
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/darrylcousins/boxesapp.git"
  },
  "keywords": [
    "shopify"
  ],
  "author": "Darryl Cousins <darryljcousins@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/darrylcousins/boxesapp/issues"
  },
  "homepage": "https://github.com/darrylcousins/boxesapp#readme",
  "devDependencies": {
    "@b9g/crank": "^0.4.3",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.8.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "sass": "^1.54.0",
    "serve-static": "^1.15.0",
    "tachyons": "^4.12.0",
    "vite": "^2.9.9",
    "vitest": "^0.19.1",
    "winston": "^3.8.1"
  },
  "dependencies": {
    "winston-mongodb": "^5.0.7"
  }
}
